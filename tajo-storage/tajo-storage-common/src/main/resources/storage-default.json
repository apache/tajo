{
  "storages": {
    "hdfs": {
      "handler": "org.apache.tajo.storage.FileTablespace",
      "default-format": "text"
    },
    "file": {
      "handler": "org.apache.tajo.storage.FileTablespace",
      "default-format": "text"
    },
    "s3": {
      "handler": "org.apache.tajo.storage.FileTablespace",
      "default-format": "text"
    },
    "hbase:zk": {
      "handler": "org.apache.tajo.storage.hbase.HBaseTablespace",
      "default-format": "hbase"
    },
    "jdbc:postgresql": {
      "handler": "org.apache.tajo.storage.pgsql.PgSQLTablespace",
      "default-format": "rowstore"
    }
  }
}